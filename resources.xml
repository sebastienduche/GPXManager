<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="copy_resources" name="Copy Resources in bin directory: Dev only">
    <!--ANT 1.7 is required                                        -->
    <target name="copy_resources">
    	<mkdir dir="./bin/resources"/>
    	<copydir src="./resources" dest="./bin/resources"/>
    </target>
</project>
